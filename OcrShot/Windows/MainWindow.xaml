<Window x:Class="OcrShot.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OcrShot"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded" Closing="Window_Closing" StateChanged="Window_StateChanged" Icon="/Resources/ocr.ico" ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="ScanButton" Content="Scan" Margin="0,20,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Height="39" Width="93" Click="ScanButton_Click"></Button>
        <ScrollViewer Width="764" Margin="19,72,17,21" Background="#FF14A8CD" CanContentScroll="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="HistoryStackPanel"/>
        </ScrollViewer>
    </Grid>
</Window>
